{
  "name": "react-boilerplate",
  "version": "0.8.2",
  "description": "React, Redux, code-splitting, hot-reload, Scss, Stage 2, ESLint, Express, ES2016",
  "main": "server.js",
  "scripts": {
    "start": "pm2-dev ./config/ecosystem.config.js",
    "start:nodemon": "NODE_ENV=development nodemon --ignore 'src/**' --ext \"graphql js\"",
    "start:prod": "pm2 start server.js",
    "build:prod": "webpack --config ./config/webpack.prod.js",
    "start:docker": "pm2-docker start ./config/ecosystem.docker.config.js",
    "test": "jest",
    "precommit": "lint-staged",
    "stats": "webpack-cli --config ./config/webpack.prod.js --json > stats.json"
  },
  "author": "nure,",
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": "precommit",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "hpp": "^0.2.2",
    "react": "^16.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "mini-css-extract-plugin": "^0.4.2",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "react-dom": "^16.5.0",
    "react-hot-loader": "^4.3.6",
    "react-router-dom": "^4.3.1",
    "resolve-url-loader": "^2.3.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.23.1",
    "webpack-merge": "^4.1.4",
    "webpack-pwa-manifest": "^3.7.1",
    "workbox-webpack-plugin": "^3.5.0"
  }
}
